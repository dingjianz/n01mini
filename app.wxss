/**app.wxss**/
@import './assets/css/icon.wxss';
@import './utils/animate.wxss';

.container {
  height: 100%;
  box-sizing: border-box;
} 
page{
    background-color:#001434;
    color: #fff;
}

.clearfix:before,
.clearfix:after{
  content: '';
  display: table;
  height: 0;
  width: 0;
}
.clearfix:after{
  clear: both;
}
.f-l{
  float: left;
}
.f-r{
  float: right;
}
.t-l{
  text-align: left
}
.t-r{
  text-align: right;
}
.t-c{
  text-align: center;
}
.t-ellipsis{
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.d-flex{display: flex;display: -webkit-flex;}
.hidden{ display: none !important; }
.vis_hid{visibility: hidden !important;}

.loading{background: url(data:image/svg+xml;base64,) no-repeat center center/55rpx 55rpx;}
.isIPX {
  padding-bottom: 68rpx !important;
}