/* pages/create-team/create-team.wxss */
.f-input{
    height: 110rpx;
    background-color: #102957;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 15rpx 30rpx;
    color: #84A5E0;
    margin-top: 30rpx;
    box-sizing: border-box;
}
.f-input .f-input-input{
    flex: 1;
    font-size: 26rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: #84A5E0;
    font-weight: 400;
}
.f-input .f-input-placeholder{
    color: #395787;
}
.f-input .f-input-title{
    font-size: 30rpx;
    height: 42rpx;
    line-height: 42rpx;
    font-weight: 400;
}
.f-tips{
    padding: 0 14rpx;
    height: 33rpx;
    line-height: 33rpx;
    font-size: 24rpx;
    color: #2E4E82;
    display: block;
    font-weight: 400;
    margin-top: 25rpx;
    text-indent: 24rpx
}
.f-button{
    width: 690rpx;
    display: block;
    margin: 137rpx auto 0;
    height: 98rpx;
    border: 2rpx solid #0D295B;
    background: #0B224A;
    border-radius: 10rpx;
    color: #456EAD;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 98rpx;
    padding: 0;
}