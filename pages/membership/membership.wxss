/* pages/membership/membership.wxss */
@import "../../assets/default-skin/cell.wxss";
.content-container{
    padding-bottom: 30rpx;
}
.custom-cell-group{
    margin: 30rpx auto 0;
    padding: 0;
    width: 710rpx;
    border-radius: 10rpx;
    overflow: hidden;
}
.custom-cell-group .custom-cell{
    border-radius: 0;
    margin-top: 0;
    position: relative;
}
/* .custom-cell-group .custom-cell::first-child{
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}
.custom-cell-group .custom-cell::last-child{
    border-bottom-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
} */
.custom-cell-group .custom-cell:after{
    height: 100%;
    width: 680rpx;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    box-sizing: border-box;
    border-bottom: 1rpx solid #001434;
}
.custom-cell-group .custom-cell .cell-title .remarks{
    width: 44rpx;
    height: 44rpx;
    border-radius: 22rpx;
    background-color: #091F44;
    line-height: 44rpx;
    text-align: center;
}
.f-button{
    display: block;
    height: 110rpx;
    margin: 0 auto 0;
    box-sizing: border-box;
    background: #102957;
    color: #456EAD;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 110rpx;
    border: 0 none;
    padding: 0;
}
.f-button::after{
    border: 0 none;
}
.f-button .iconfont{
    font-size: 32rpx;
    /* vertical-align: middle */
}