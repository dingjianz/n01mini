/* pages/membership/membership.wxss */
@import "../../assets/default-skin/cell.wxss";

.custom-cell-group{
    padding: 0;
    margin-top: 30rpx;
}
.custom-cell-group .custom-cell{
    border-radius: 0;
    margin-top: 0;
    position: relative;
}
.custom-cell-group .custom-cell:after{
    height: 100%;
    width: 710rpx;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    box-sizing: border-box;
    border-bottom: 1rpx solid #001434;
}

.f-button{
    display: block;
    height: 98rpx;
    width: 100%;
    border: 2rpx solid #0D295B;
    background: #0B224A;
    color: #456EAD;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 98rpx;
    padding: 0;
    position: absolute;
    bottom: 0;
    left: 0;
    border-radius: 0;
}