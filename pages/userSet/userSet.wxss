/* pages/userSet/userSet.wxss */
@import "../../assets/default-skin/cell.wxss";

.custom-cell-group{
    margin: 30rpx auto 140rpx;
    padding: 0;
    width: 710rpx;
    border-radius: 10rpx;
    overflow: hidden;
}
.custom-cell-group .custom-cell{
    border-radius: 0;
    margin-top: 0;
    position: relative;
}
.custom-cell-group .custom-cell:after{
    height: 100%;
    width: 680rpx;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    box-sizing: border-box;
    border-bottom: 1rpx solid #001434;
}
.custom-cell-group .custom-cell .cell-title .remarks{
    width: 44rpx;
    height: 44rpx;
    border-radius: 22rpx;
    background-color: #091F44;
    line-height: 44rpx;
    text-align: center;
}
.f-button{
    display: block;
    height: 98rpx;
    margin: 0 auto 0;
    box-sizing: border-box;
    background: #102957;
    color: #8C1F1F;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 98rpx;
    border: 0 none;
    padding: 0;
}
.f-button::after{
    border: 0 none;
}
.f-button .iconfont{
    width:30rpx;
    font-size: 32rpx;
    vertical-align: middle
}