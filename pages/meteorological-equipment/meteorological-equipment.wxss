.container {
  padding: 31rpx 19rpx 20rpx 21rpx;
}

.euip-wrapper {
  background: rgba(16, 41, 87, 1);
  border-radius: 10rpx;
  padding: 28rpx 21rpx 20rpx 19rpx;
}

.euip-wrapper .equip-title {
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
}

.euip-wrapper .equip-title {
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 48rpx;
}

.euip-wrapper .equip-title .euip-name {
  height: 42rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(21, 233, 252, 1);
  line-height: 40rpx;
}

.euip-wrapper .equip-title .euip-basePosition {
  display: inline-block;
  width: 173rpx;
  height: 38rpx;
  text-align: center;
  background: rgba(11, 34, 74, 1);
  border-radius: 19rpx;
  font-size: 24rpx;
  line-height: 33rpx;
  font-weight: 400;
  color: rgba(57, 87, 135, 1);
  margin-left: 10rpx;
  vertical-align: middle;
}

.euip-wrapper .equip-title .iconshezhi {
  font-size: 36rpx;
  color: #314C75;
}

/* EC值列表 */
.euip-wrapper .ec-value .val-wrapper {
  width: 320rpx;
  height: 120rpx;
  box-sizing: border-box;
  border: 1rpx solid rgba(39, 63, 100, 1);
  margin-left: 8rpx;
}

.euip-wrapper .ec-value .col-class {
  margin-bottom: 30rpx;
}

.euip-wrapper .ec-value .val-wrapper {
  width: 320rpx;
  height: 120rpx;
  box-sizing: border-box;
  border: 1rpx solid rgba(39, 63, 100, 1);
  border-radius: 10rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  padding: 40rpx 30rpx;
}

.euip-wrapper .ec-value .val-wrapper .val-name {
  height: 40rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  font-weight: 400;
  color: rgba(139, 171, 231, 1);
}

.euip-wrapper .ec-value .val-wrapper .val-unit {
  height: 33rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(21, 252, 113, 1);
  line-height: 33rpx;
}

/* 设备开关 */
.euip-switch-list .euip-switch-item {
  width: 670rpx;
  height: 120rpx;
  box-sizing: border-box;
  border: 1rpx solid rgba(39, 63, 100, 1);
  border-radius: 10rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  margin-bottom: 30rpx;
}

.euip-switch-list .euip-switch-item .switch-name {
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(139, 171, 231, 1);
  line-height: 40rpx;
}
.euip-switch-list .euip-switch-item .switch-onoff {
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
}
.euip-switch-list .euip-switch-item .switch-onoff .iconjiantou{
  font-size: 26rpx;
  color:#354E75;
  padding-left: 35rpx;
  margin-right: -14rpx;
}