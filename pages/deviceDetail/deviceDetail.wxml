<view class="container">
  <view class="device-status">
    <view class="status-top">
      <view class="device-positionDes">生产示范基地</view>
      <icon class="iconfont iconshezhi"></icon>
    </view>
    <view class="status-middle">
      开启
    </view>
    <view class="status-bottom">内遮阳一号开关</view>
  </view>
  <view class="control-log">
    <view class="control-title">操作记录</view>
    <view class="divde-border"></view>

    <view class="control-detail">
      212212

      <block wx:for="{{axis}}" wx:key="*this">
        <view class='weui-cell-list'>

          <view class='weui-cell-circle'></view>

          <view class="weui-cell-line {{index === (axis.length - 1) ? 'lineHidden' : ''}}" >

            <view class='weui-cell-time'>{{item.time}}</view>
            <view class='weui-cell-name'>{{item.name}}</view>
            <view class='weui-cell-event'>{{item.event}}</view>
          </view>

        </view>

      </block>
    </view>
  </view>
</view>