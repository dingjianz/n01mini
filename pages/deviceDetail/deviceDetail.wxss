
.container {
  min-height:100vh;
  display: inline-block;
  padding: 31rpx 19rpx 0 21rpx;
  color: #385076;
  font-size: 24rpx;
}

.container.IPX{
  margin-bottom: 18rpx;
}

.device-status {
  width: 710rpx;
  height: 300rpx;
  overflow-x: hidden;
  box-sizing: border-box;
  background-color: #102957;
  border-radius: 10rpx;
  position: relative;
}


.device-status .status-top {
  width: 710rpx;
  height: 66rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: flex-end;
  padding: 0 20rpx;
}

.device-status .status-top .device-positionDes {
  height: 38rpx;
  color: #395787;
  background-color: #0B224A;
  border-radius: 19rpx;
  padding: 0 15rpx;
  font-size: 24rpx;
  line-height: 38rpx;
}

.device-status .status-top .iconshezhi {
  font-size: 36rpx;
  margin: -10rpx 38rpx 0 0;
}

.device-status .status-middle {
  width: 144rpx;
  height: 144rpx;
  border-radius: 50%;
  margin: 0 auto;
  box-sizing: border-box;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  font-size: 42rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 59rpx;
  margin-top: 6rpx;
}

.device-status .car-wrapper {
  min-width:330rpx;
  height: 160rpx;
  box-sizing: border-box; 
  border-radius: 10rpx;
  border: 1rpx solid rgba(39, 63, 100, 1);
  display: inline-block;
  margin-top: 33rpx;
  padding: 20rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  transform: translate(-50%,0);
}

.device-status .car-wrapper .car-des {
  font-size: 26rpx;
  font-weight: 400;
  color: #8BABE7;
  margin-top: 9rpx;
  max-width: 330rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.device-status .car-wrapper .car-des.car-des-numType {
  margin-top: 15rpx;
}

.z-status-open {
  background: linear-gradient(360deg, rgba(29, 197, 37, 1) 0%, rgba(37, 240, 132, 1) 100%);
  border: 1rpx solid rgba(46, 215, 81, 1);
}

.z-status-close {
  background: linear-gradient(360deg, rgba(254, 77, 77, 1) 0%, rgba(252, 136, 109, 1) 100%);
  border: 1rpx solid rgba(220, 51, 51, 1);
}

.z-status-pause {
  background: linear-gradient(360deg, rgba(254, 148, 77, 1) 0%, rgba(254, 189, 77, 1) 100%);
  border: 1rpx solid rgba(220, 116, 31, 1);
}

.device-status .status-bottom {
  text-align: center;
  height: 36rpx;
  font-size: 25rpx;
  line-height: 36rpx;
  color: #8BABE7;
  font-weight: 400;
  margin-top: 12rpx;
}

.control-log {
  width: 710rpx;
  position: relative;
  min-height: calc(100vh - 390rpx);
  box-sizing: border-box;
  padding-bottom: 15rpx;
  background-color: #102957;
  border-radius: 10rpx;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.control-log .control-title {
  height: 108rpx;
  margin-left: 30rpx;
  line-height: 108rpx;
  font-size: 28rpx;
  color: #15E9FC;
  font-weight: 400;
}

.control-log .divde-border {
  margin: 0 31rpx 0 29rpx;
  border-bottom: 1rpx solid rgba(0, 20, 52, 1);
}

.control-detail {
  /* display: flex;
  display: -webkit-flex;
  justify-content: space-between; */
  /* height:calc(100% - 439rpx); */
}

.control-detail .log-detail {
  margin-top: 41rpx;
}
.control-detail .log-detail .log-wrapper {
  width: 604rpx;
  height: 140rpx;
  box-sizing: border-box;
  padding: 30rpx;
  color: #456EAD;
  background:rgba(0,20,52,0.2);
  border-radius: 10rpx;
  padding: 30rpx 0 0 30rpx;
  margin: 0 30rpx 40rpx 76rpx;
  position: relative;
}

.log-wrapper .log-time {
  height: 37rpx;
  font-size: 26rpx;
  font-weight: 400;
  line-height: 37rpx;
  margin-bottom: 6rpx;
}

.log-wrapper .log-status {
  height: 37rpx;
  font-size: 26rpx;
  font-weight: 400;
}

.log-wrapper .z-open {
  color: #8BABE7;
  font-size: 26rpx;
  line-height: 37rpx;
  font-weight: 400;
}

.log-wrapper::before{
  content: "";
  width: 12rpx;
  height: 12rpx;
  background-color: rgba(21, 233, 252, 1);
  border-radius: 50%;
  display: inline-block;
  position: absolute;
  top: 10rpx;
  left: -43rpx;
}
.log-wrapper::after{
  content: "";
  width: 2rpx;
  height: 150rpx;
  background-color:#2E4973;
  display: inline-block;
  position: absolute;
  top: 30rpx;
  left: -39rpx;
}
.lineHidden::after{
  content: "";
  height: 0;
}
.no-data-ww{
  position: absolute;
  width: 100%;
  height: calc(100% - 110rpx);
  top: 110rpx;
  left: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}