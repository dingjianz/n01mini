/* pages/teamSet/teamSet.wxss */
@import "../../assets/default-skin/cell.wxss";

.custom-cell-group{
    margin: 30rpx auto 20rpx;
    padding: 0;
    width: 710rpx;
    border-radius: 10rpx;
    overflow: hidden;
}
.custom-cell-group.mb140{
    margin: 30rpx auto 140rpx;
}
.custom-cell-group .custom-cell{
    border-radius: 0;
    margin-top: 0;
    position: relative;
}
.custom-cell-group .custom-cell .iconfont{
    margin-left:10rpx;
}
.custom-cell-group .custom-cell:after{
    height: 100%;
    width: 680rpx;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    box-sizing: border-box;
    border-bottom: 1rpx solid #001434;
}
.cell-footer-wrap .cname{
    color:#456EAD;
}
.f-button{
    display: block;
    height: 98rpx;
    margin: 0 auto 0;
    box-sizing: border-box;
    background: #102957;
    color: #6F1E1E;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 98rpx;
    border: 0 none;
    padding: 0;
}
.f-button.handover{
    color:#25538E;
}
.f-button::after{
    border: 0 none;
}
.f-button .iconfont{
    width:30rpx;
    font-size: 32rpx;
    vertical-align: middle;
    margin-right:14rpx;
}
.f-input-label{
  width: 400rpx;
  height: 80rpx;
  margin: 0 auto;
  box-sizing: border-box;
  border: 1rpx solid #294166;
  background-color: transparent;
  padding: 10rpx 20rpx;
  display: flex;
  align-items: center;
  border-radius: 8rpx;
  margin-bottom: 23rpx;
}
.f-input-label .f-input{
  flex: 1;
  font-size: 26rpx;
  line-height: 50rpx;
  height: 50rpx;
  display: block;
  color: #15E9FC;
  border: 0 none;
  outline: none;
  background-color: transparent;
}
.f-input-label .f-input .f-input-placeholder{
  color: #3F639C;
}
.i-modal-body{
    height:138rpx!important;
}
.i-modal-body .errortip{
    color:#7A2221;
    text-align:center;
    display:block;
    height:24rpx;
    line-height:24rpx;
    width:100%;
}
.i-modal-body .modaltip{
    color:#4974B6;
}