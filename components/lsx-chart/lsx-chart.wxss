.wind-log {
  background: rgba(16, 41, 87, 1);
  border-radius: 10rpx;
  margin-top: 30rpx;
  padding-right: 10rpx;
  padding-bottom: 30rpx;
}


.log-wrapper {
  width: 604rpx;
  height: 140rpx;
  color: #456EAD;
  box-sizing: border-box;
  background: rgba(0, 20, 52, 0.2);
  border-radius: 10rpx;
  padding: 30rpx 0 0 30rpx;
  margin: 0 30rpx 40rpx 76rpx;
  position: relative;
}

.log-wrapper .log-time {
  height: 37rpx;
  font-size: 26rpx;
  font-weight: 400;
  line-height: 37rpx;
}

.log-wrapper .log-status {
  height: 37rpx;
  font-size: 26rpx;
  font-weight: 400;
  line-height: 37rpx;
  color:#8BABE7;
}

.log-wrapper::before{
  content: "";
  width: 12rpx;
  height: 12rpx;
  background-color: rgba(21, 233, 252, 1);
  border-radius: 50%;
  display: inline-block;
  position: absolute;
  top: 10rpx;
  left: -43rpx;
}
.log-wrapper::after{
  content: "";
  width: 2rpx;
  height: 150rpx;
  background-color:#2E4973;
  display: inline-block;
  position: absolute;
  top: 30rpx;
  left: -39rpx;
}
.lineHidden::after{
  content: "";
  height: 0;
}