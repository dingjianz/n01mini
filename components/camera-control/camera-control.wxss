.control-container .ctrl-scale{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    display: flex;
    justify-content: space-between;
}
.control-container .ctrl-scale .scale-key{
    width: 75rpx;
    height: 75rpx;
    background-size: 100% 100% !important;
    margin: 0;
    padding: 0;
}
.control-container .ctrl-scale .scale-key.zoom-out{
    background: url(data:image/png;base64,)no-repeat;
}
.control-container .ctrl-scale .scale-key.zoom-out.active{
    background: url(data:image/png;base64,)no-repeat;
}
.control-container .ctrl-scale .scale-key.zoom-in{
    background: url(data:image/png;base64,)no-repeat;
}
.control-container .ctrl-scale .scale-key.zoom-in.active{
    background: url(data:image/png;base64,)no-repeat;
}