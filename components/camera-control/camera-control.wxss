/* components/camera-control/camera-control.wxss */
@import "../../assets/css/icon.wxss";
button {
    margin: 0;
    padding: 0;
    border: 0 none;
    background-color: transparent;
    outline: none;
}
button::after{
    border: none;
}
.control-container{
    height: 312rpx;
    width: 312rpx;
    background-color: rgba(0,0,0,.2);
    position: relative;
    color: #fff;
    font-size: 20rpx;
}
.control-container .ctrl-close{
    position: absolute;
    top: 0;
    right: 8rpx;
    width: 56rpx;
    height: 56rpx;
    border-radius: 28rpx;
    background:rgba(85,85,85,0.5);
    display: flex;
    align-items: center;
    justify-content: center;
}
.control-container .ctrl-close .close-icon{
    font-size: 24rpx;
    color: #fff;
    font-weight: 400;
}
.control-container .ctrl-steering-wheel{
    width: 202rpx;
    height: 202rpx;
    border-radius: 101rpx;
    overflow: hidden;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    background: url(data:image/png;base64,)no-repeat;
    background-size: 100% 100%;
    top: 28rpx;
}
.control-container .ctrl-steering-wheel .arrow-key{
    position: absolute;
    /* background-size: 100% 100% !important; */
    background-color: transparent;
    padding: 0;
    margin: 0;
    width: 50rpx;
    height: 50rpx;
    /* background-color: #fff !important; */
    background-position: center center !important;
}
.control-container .ctrl-steering-wheel .arrow-key.key-top,
.control-container .ctrl-steering-wheel .arrow-key.key-btm{
    background-size: 27rpx 16rpx;
    left: 50%;
    transform: translateX(-50%);
}
.control-container .ctrl-steering-wheel .arrow-key.key-right,
.control-container .ctrl-steering-wheel .arrow-key.key-left{
    top: 50%;
    transform: translateY(-50%);
    background-size: 16rpx 27rpx;
}
.control-container .ctrl-steering-wheel .arrow-key.key-top{
    top: 15rpx;
    background: url(data:image/png;base64,)no-repeat;
}
.control-container .ctrl-steering-wheel .arrow-key.key-top.active{
    background: url(data:image/png;base64,)no-repeat;
}

.control-container .ctrl-steering-wheel .arrow-key.key-right{
    right: 15rpx;
    background: url(data:image/png;base64,)no-repeat;
}
.control-container .ctrl-steering-wheel .arrow-key.key-right.active{
    background: url(data:image/png;base64,)no-repeat;
}
.control-container .ctrl-steering-wheel .arrow-key.key-btm{
    bottom: 15rpx;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYBAMAAAASWSDLAAAAMFBMVEVHcExueIJ+iJgAAAaDjZ0fISiDjJ1gYHgfISlze4sDAwYAAAdESVODjZ15g5I5PUdyEqRqAAAADXRSTlMAGdofkN5rC97RXCPOciZbdQAAAEdJREFUGNNjYKAlYBMAkekQTq4ikGC5WQDmCF0CSvm+MABzGHUVGVjuBUM1AaV8nxpAOYy6KnAJoNRduARI6jeSVTILGOgJAO7zDfj9M8wpAAAAAElFTkSuQmCC)no-repeat;
}
.control-container .ctrl-steering-wheel .arrow-key.key-btm.active{
    background: url(data:image/png;base64,)no-repeat;
}
.control-container .ctrl-steering-wheel .arrow-key.key-left{
    left: 15rpx;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYBAMAAAASWSDLAAAAMFBMVEVHcEyDiZ2DjZ1qcX94gpF/iJgJCw91fo0AAAADAwZ0foxxeIc0OECDjZ1haHVCR1IoGRPBAAAADXRSTlMAJ+VFivunWRpYiqy8X8mFVgAAADhJREFUGNNjYCAbMBYgcYSuI0noPkOSuJdAogTL3QNIBu8NRuJwXTVA4vkiS7HgkfqJ7OwGBmoAAJEvDr3b2bajAAAAAElFTkSuQmCC)no-repeat
